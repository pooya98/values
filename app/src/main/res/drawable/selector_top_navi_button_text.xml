<?xml version="1.0" encoding="utf-8"?>

<!-- 상단 네비게이션 바 [버튼 텍스트 색]-->
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <!-- 버튼 텍스트 색(선택 되었을 경우) -->
    <item android:color="@color/dh_white" android:state_checked="true" />
    <!-- 버튼 텍스트 색(선택 되지 않은 경우) -->
    <item android:color="#646464" android:state_checked="false" />
</selector>